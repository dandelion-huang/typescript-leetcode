{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb-typescript/base", "plugin:prettier/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/lines-between-class-members": "off",
        "import/prefer-default-export": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off"
    }
}
